{
    "name": "f-bahesna/intervention-image-wrapper",
    "description": "Simple Laravel wrapper for Intervention Image v3.",
    "type" : "library",
    "keywords": ["image-intervention", "laravel-intervention-image"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lianum Frada Bahesna",
            "email": "fbahezna@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "intervention/image": "^3.0",
      "ext-http": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Fbahesna\\InterventionImageWrapper\\": "src/",
            "Tests\\" : "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fbahesna\\InterventionImageWrapper\\ImageWrapperServiceProvider"
            ],
            "aliases": {
                "ImageWrap" : "ImageWrapper"
            }
        }
    }
}
